      <bpmn:incoming>Flow_05</bpmn:incoming>
      <bpmn:outgoing>Flow_End</bpmn:outgoing>
    </bpmn:task>

    <bpmn:endEvent id="EndEvent_uc200" name="Nutzung für Betrieb und Erhaltung abgeschlossen">
      <bpmn:documentation>Lieferobjekte:
- CAFM-Datensatz
- Betriebsdokumentation
- Aktualisierte Bestandsdaten</bpmn:documentation>
      <bpmn:incoming>Flow_End</bpmn:incoming>
    </bpmn:endEvent>

