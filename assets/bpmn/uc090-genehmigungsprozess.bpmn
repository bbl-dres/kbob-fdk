    <bpmn:sequenceFlow id="Flow_Gateway1_Ja" name="Ja - Vollständig und geprüft" sourceRef="Gateway_Vollstaendig" targetRef="Task_07" />
    <bpmn:sequenceFlow id="Flow_Gateway1_Nein" name="Nein - Unterlagen fehlen" sourceRef="Gateway_Vollstaendig" targetRef="Task_04">
      <bpmn:documentation>Bei fehlenden Unterlagen erfolgt Rückkopplung zu den Fachplanern für Nacharbeiten.</bpmn:documentation>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_07" sourceRef="Task_07" targetRef="Task_08" />
    <bpmn:sequenceFlow id="Flow_08" sourceRef="Task_08" targetRef="Task_09" />
    <bpmn:sequenceFlow id="Flow_09" sourceRef="Task_09" targetRef="Gateway_Bewilligung" />
    <bpmn:sequenceFlow id="Flow_Gateway2_Ja" name="Ja - Bewilligung erteilt" sourceRef="Gateway_Bewilligung" targetRef="Task_12" />
    <bpmn:sequenceFlow id="Flow_Gateway2_Nein" name="Nein - Nachforderungen/Ablehnung" sourceRef="Gateway_Bewilligung" targetRef="Task_10">
      <bpmn:documentation>Bei Nachforderungen oder Ablehnung werden die Unterlagen überarbeitet und ergänzt.</bpmn:documentation>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_Loop" sourceRef="Task_10" targetRef="Task_11" />
    <bpmn:sequenceFlow id="Flow_11" sourceRef="Task_11" targetRef="Task_07">
      <bpmn:documentation>Nach Überarbeitung der Fachplaner-Unterlagen erfolgt die erneute Einreichung.</bpmn:documentation>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_12" sourceRef="Task_12" targetRef="Task_13" />
    <bpmn:sequenceFlow id="Flow_13" sourceRef="Task_13" targetRef="Task_14" />
    <bpmn:sequenceFlow id="Flow_End" sourceRef="Task_14" targetRef="EndEvent_uc090" />

  </bpmn:process>

