        <bpmn:flowNodeRef>Task_05</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_06</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Vollstaendig</bpmn:flowNodeRef>
