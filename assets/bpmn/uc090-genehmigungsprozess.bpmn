        <bpmn:flowNodeRef>Task_12</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_14</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_uc090</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Architekt" name="Architekt/Gesamtleiter">
